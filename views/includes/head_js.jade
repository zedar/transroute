script.
  require(['dojo/query', 'dojo/parser', 
    'bootstrap/Support',
    'bootstrap/Dropdown', 
    'bootstrap/Collapse',  
    'bootstrap/Carousel',
    'dojo/domReady!'], function(query, parser, support) {
    parser.parse();
    query('.dropdown-toggle').dropdown();
    query('.collapse').collapse({
      toggle: false
    });

    query('a.dropdown-toggle, .dropdown-menu a').on('touchstart', function(e) {
      e.stopPropagation();
    });

    query(".carousel").carousel({
      interval: 10000
    });
  });